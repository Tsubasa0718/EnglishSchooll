<footer class="footer theme-footer-bg">
     <div class="container">
      <div class="footer__wrapper">
        <% footerData.forEach(category => { %>
          <div class="footer__nav" role="navigation" aria-labelledby="<%= category.id %>-title">
            <h4 class="footer__list-title" id="<%= category.id %>-title">
              <%= category.title %>
          </h4>
          <ul class="footer__nav-list" aria-labelledby="<%= category.id %>-title">
              <% category.links.forEach(item => { %>
                  <li class="footer__list-item">
                      <a class="footer__nav-link" href="<%= item.link %>">
                          <%= item.title %>
                      </a>
                  </li>
              <% }) %>
          </ul>

          </div>
        <% }) %>
      </div>
     </div>
  </footer>
